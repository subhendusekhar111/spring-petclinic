node('JDK_11') {
    stage('SourceCode') {
        git branch: '*/scripted', url: 'https://github.com/subhendusekhar111/spring-petclinic.git'
    }

    stage('<Build the code') {
        sh 'mvn package'

    }
    stage('arctive artifact and test result') {
        junit '**/surefire-reports/*.xml'
        archiveArtifacts artifacts: '**/*.war', followSymlinks: false

    }
}